generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Payment_Info{
  index_id String @unique
  gateway String
  order_id    String
  gatewayId String
  amount  Int
  currency String
  paymentStatus String
  @@map("Payment_Info")
}